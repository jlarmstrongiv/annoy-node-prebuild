{
  "name": "annoy-node",
  "version": "0.1.0",
  "description": "Annoy Indexing binidings for node.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc -b",
    "install": "prebuild-install --runtime napi || cmake-js rebuild",
    "rebuild": "cmake-js rebuild",
    "prebuild": "prebuild --runtime napi --all --strip --verbose --backend cmake-js",
    "upload": "prebuild --runtime napi --upload ${GITHUB_TOKEN}",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prathameshnetake/annoy-node.git"
  },
  "keywords": [],
  "author": "Prathamesh Netake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prathameshnetake/annoy-node/issues"
  },
  "homepage": "https://github.com/prathameshnetake/annoy-node#readme",
  "devDependencies": {
    "@types/node": "^15.6.1",
    "node-addon-api": "^3.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "binary": {
    "napi_versions": [
      6,
      7,
      8
    ]
  }
}
